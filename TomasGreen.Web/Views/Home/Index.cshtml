@inject CustomLocalizer localizer

@{
    ViewData["Title"] = "Home Page";
}


<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active center-block">
            
            <img src="~/images/Pistachios.png" alt="Pistachios" class="img-responsive center-block" />
            
        </div>
        <div class="item center-block">
            <img src="~/images/Peanut.png" alt="Peanut" class="img-responsive center-block" />
            
        </div>
        <div class="item center-block">

            <img src="~/images/Sunflower.png" alt="Sunflower" class="img-responsive center-block" />
            
        </div>
        <div class="item center-block">
            <img src="~/images/Hazelnut.png" alt="Hazelnut" class="img-responsive center-block"/>
            
        </div>
        <div class="item center-block" >
            <img src="~/images/Cashew.png" alt="Cashew" class="img-responsive center-block"/>
           
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div class="row">
    
</div>
